<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Property type</th>
      <th>Ad type</th>
      <th>Title</th>
      <th>Price</th>
      <th>Area</th>
      <th>Description</th>
      <th>Address</th>
      <th>City</th>
      <th>Image1</th>
      <th>Image2</th>
      <th>Image3</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.property_type %></td>
        <td><%= property.ad_type %></td>
        <td><%= property.title %></td>
        <td><%= property.price %></td>
        <td><%= property.area %></td>
        <td><%= property.description %></td>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.image1 %></td>
        <td><%= property.image2 %></td>
        <td><%= property.image3 %></td>
        <td><%= property.latitude %></td>
        <td><%= property.longitude %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>

<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Type of property</th>
      <th>Price</th>
      <th>Area</th>
      <th>Image1 url</th>
      <th>Image2 url</th>
      <th>Image3 url</th>
      <th>Description</th>
      <th>Address</th>
      <th>City</th>
      <th>Type of transaction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.title %></td>
        <td><%= property.type_of_property %></td>
        <td><%= property.price %></td>
        <td><%= property.area %></td>
        <td><%= property.image1_url %></td>
        <td><%= property.image2_url %></td>
        <td><%= property.image3_url %></td>
        <td><%= property.description %></td>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.type_of_transaction %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
